<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  
	<persistence-unit name="tremble_persistence_unit">
	
		<provider>oracle.toplink.essentials.PersistenceProvider</provider>
		
		<class>Model.Course</class>
		<class>Model.Reg_Session</class>
		<class>Model.Sess_Class</class>
		<class>Model.Trainee</class>
		<class>Model.Trainer</class>
		<class>Model.Wave_Date</class>
		<class>Model.Wave</class>
		<class>Model.Zone</class>
		<class>Model.Location</class>
		<class>Model.Active_Session</class>
		<class>Model.Answer</class>
		<class>Model.Question</class>

		<properties>
			<property name="toplink.jdbc.user" value="root"/>
			<property name="toplink.jdbc.password" value="root"/>
			<property name="toplink.jdbc.url" value="jdbc:mysql://localhost:3306/new_schema"/>
			<property name="toplink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
			<!--  <property name="toplink.ddl-generation" value="create-tables"/> -->
		</properties>
	
	</persistence-unit>
	
</persistence>